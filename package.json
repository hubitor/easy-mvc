    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.2"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      [
        "component",
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ],
    "comments": false,
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
