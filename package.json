    "element-ui": "^2.3.6",
    "hapi": "^17.3.1",
    "inert": "^5.1.0",
    "joi": "^13.2.0",
    "knex": "^0.14.6",
    "mysql2": "^1.5.3",
    "objection": "^1.1.7",
    "require-directory": "^2.1.1",
    "vision": "^5.3.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
